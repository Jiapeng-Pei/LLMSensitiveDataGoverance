{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.CommandLine": "Warning"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "SensitivityLabels": {
    "RepositoryType": "Json",
    "ConfigurationPath": "./config/labels.json",
    "DefaultEncryption": {
      "Algorithm": "AES256",
      "KeySize": 256
    },
    "ValidationRules": {
      "RequireDescription": true,
      "MinDescriptionLength": 10,
      "MaxNameLength": 100,
      "AllowDuplicateNames": false
    }
  },
  "CLI": {
    "OutputFormat": "table",
    "EnableColors": true,
    "PageSize": 25,
    "DefaultTimeout": 30000,
    "TempDirectory": "./temp"
  }
}